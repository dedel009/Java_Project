package project0104;
import java.util.*;

public class InputStr {
	static Scanner sc = new Scanner(System.in);
	
	public void subStr() {
		System.out.println("글자입력");
		String input = sc.next();
		Str str = new Str();
		str.str = input;
		InstanceArray ia = new InstanceArray();
		ia.str = new Str[3];
	}
}
