package project1230;
import java.util.*;

public class Coding_8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		PhoneNumberBook pnb = new PhoneNumberBook();
		while(true) {
			Scanner(pnb);
		}
	}
	
	public static void Scanner(PhoneNumberBook pnb) {
		try {
			
			Scanner sc = new Scanner(System.in);
			System.out.println("[1]전화번호부 [2]전화걸기 [3]시간확인 [4]인터넷 검색");
			int menu = sc.nextInt();
			switch(menu) {
			case 1:
				System.out.println("전화번호부 : ");
				for(String s: pnb.getPhonenumber_array()) {
					System.out.println(s);
				}
				break;
			case 2:
				System.out.println("1.영수 2.진수 3.길동");
				int num = sc.nextInt();
				System.out.println(pnb.getPhonenumber_array()[num]);
			case 3:
			case 4:
			}
		}catch(Exception e) {
			System.out.println(e);
		}
	}

}
