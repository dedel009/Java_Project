package project1230;

public class Coding_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "9B768C";
		String array[] = str.split("");
		int sum =0;
		char c = 0;
		for(String s : array) {
			if(s.equals("B")||s.equals("C")) {
				c = s.charAt(0);
				sum += c;
			}else {
				sum += Integer.parseInt(s);
			}
		}
		System.out.println(sum);
	}

}
