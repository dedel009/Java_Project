package project0104;
import java.util.*;

public class SubMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		InputStr is = new InputStr();
		
		while(true) {
			System.out.println("[1]입력 [2]조회");
			int menu = sc.nextInt();
			switch(menu) {
			case 1:
				is.input(is);
				break;
			case 2:

				break;
			}
		}
	}
	
	public static void print(Str[] str_array) {
		for(int i=0; i<str_array.length; i++) {
			System.out.println(str_array[i].str);
		}
	}

}
