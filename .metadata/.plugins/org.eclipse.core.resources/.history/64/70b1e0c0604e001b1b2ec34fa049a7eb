package project0104;
import java.util.*;

public class InputStr {
	static Scanner sc = new Scanner(System.in);
	
	public void input() {
		System.out.println("글자입력");
		String input = sc.next();
		Str str = new Str();
		str.str = input;
		Str_list sl = new Str_list();
		
		for(int i =0; i<sl.str_array.length; i++) {
			sl.str_array[i] = str;
		}
	}
}
